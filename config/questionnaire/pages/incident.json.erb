"own_organisation": {
  "id": "own_organisation",
  "type": "single",
  "tooltip": "Many seizures are made in collaboration with other agencies, sanctuaries, or law enforcement bodies. Please let us know what other organizations are involved with this incident.",
  "question": "Are you reporting in collaboration with another Agency?",
  "answers": ["Yes", "No"],
  "required": true
},
"new_agency_contact": {
  "id": "new_agency_contact",
  "required": true,
  "type": "form",
  "show_if": {"question": "own_organisation", "answer": "Yes"},
  "question": "Agency contact details"
},
"date_of_receipt": {
  "id": "date_of_receipt",
  "required": true,
  "show_if": {"question": "own_organisation", "answer": "Yes"},
  "type": "date",
  "question": "Date report received by other agency"
},
"date_of_discovery": {
  "id": "date_of_discovery",
  "required": true,
  "tooltip": "If you have seen this ape multiple times, please enter the date of the first sighting.",
  "type": "date",
  "question": "Date of discovery"
},
"numeric_example": {
  "id": "numeric_example",
  "type": "decimal_numeric",
  "question": "Test numeric"
},
"country_of_discovery": {
  "id": "country_of_discovery",
  "required": true,
  "type": "select",
  "tooltip": "Country of discovery may not necessarily be country of origin.",
  "question": "Country of discovery",
  "options": <%= partial("incident/countries") %>
},

<%= partial("incident/regions") %>,

"location_coords": {
  "id": "location_coords",
  "type": "gps",
  "tooltip": "Please enter numerical values only.",
  "question": "Location of discovery: GPS coordinates (optional)"
},
"type_of_location": {
  "id": "type_of_location",
  "required": true,
  "type": "single",
  "question": "Type of location",
  "answers": [
    "Wild",
    "Private premises",
    "Airport",
    "In transit"
  ],
  "other": true
},
"confiscated": {
  "id": "confiscated",
  "required": true,
  "type": "single",
  "question": "Were findings confiscated by law enforcement?",
  "answers": [
    "Yes",
    "No",
    "Don't know"
  ]
},
"quantities": {
  "id": "quantities",
  "required": true,
  "type": "quantities",
  "question": "Status of the ape(s) / Quantity"
}
